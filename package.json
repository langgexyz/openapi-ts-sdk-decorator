{
  "name": "openapi-ts-sdk-decorator",
  "version": "1.0.9",
  "description": "OpenAPI TypeScript SDK decorators with parameter-based design - provides type-safe decorator system with automatic path/query parameter handling",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src",
    "index.ts",
    "README.md",
    "tsconfig.json",
    "tsconfig.*.json"
  ],
  "keywords": [
    "openapi",
    "typescript",
    "typescript-5",
    "decorators",
    "stage3-decorators",
    "legacy-decorators",
    "sdk",
    "validation",
    "generator",
    "http",
    "rest",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langgexyz/openapi-ts-sdk-decorator.git"
  },
  "bugs": {
    "url": "https://github.com/langgexyz/openapi-ts-sdk-decorator/issues"
  },
  "homepage": "https://github.com/langgexyz/openapi-ts-sdk-decorator#readme",
  "author": "langgexyz",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "prebuild": "npm run clean",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rm -rf dist node_modules/.cache coverage",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:decorators": "jest tests/decorators",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "tsc --noEmit --strict"
  },
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "openapi-ts-sdk": "https://github.com/langgexyz/openapi-ts-sdk.git#semver:^1.0.0",
    "reflect-metadata": "^0.1.13"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0 <6.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
